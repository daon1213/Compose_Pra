<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Compose_Pra"
        tools:targetApi="31">
        <activity
            android:name=".news_app.NewsAppActivity"
            android:exported="false"
            android:label="@string/title_activity_news_app"
            android:theme="@style/Theme.Compose_Pra">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".gmail_app.GmailCloneActivity"
            android:exported="true"
            android:label="@string/title_activity_gmail_clone"
            android:theme="@style/Theme.Compose_Pra">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".compose_basics.StateAndRecompositionActivity"
            android:exported="false"
            android:label="@string/title_activity_state_and_recomposition"
            android:theme="@style/Theme.Compose_Pra">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".compose_basics.DogProfileActivity"
            android:exported="true"
            android:label="Compose_Pra"
            android:theme="@style/Theme.Compose_Pra.NoActionBar" />
    </application>

</manifest>